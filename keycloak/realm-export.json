{
  "realm": "myrealm",
  "enabled": true,
  "roles": {
    "realm": [
      { "name": "my-role",
        "description": "Access to frontend and calendar services",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "clients": [
    {
      "clientId": "frontend-app",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "secret": "secret",
      "redirectUris": ["http://localhost:8080/*"],
      "webOrigins": ["http://localhost:8080"],
      "defaultClientScopes": ["web-origins", "roles", "profile", "email"],
      "optionalClientScopes": ["address", "phone"],
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true
    }
  ],
  "users": [
    {
      "username": "demo",
      "firstName": "demo",
      "lastName": "demo",
      "email": "somemail@gmail.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "default-roles-myrealm",
        "my-role",
        "offline_access",
        "uma_authorization"
      ]
    },
    {
      "username": "igor",
      "firstName": "igor",
      "lastName": "igor",
      "email": "igor@gmail.com",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ],
      "realmRoles": [
        "default-roles-myrealm",
        "offline_access",
        "uma_authorization"
      ]
    }
  ]
}